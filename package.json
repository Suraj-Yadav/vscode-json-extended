{
	"name": "json-extended",
	"displayName": "JSON Extended",
	"description": "Gives extra information about json",
	"version": "0.0.3",
	"repository": {
		"type": "git",
		"url": "https://github.com/Suraj-Yadav/vscode-json-extended.git"
	},
	"engines": {
		"vscode": "^1.28.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "Suraj-Yadav",
	"activationEvents": [
		"onLanguage:json",
		"onLanguage:http"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"event-stream": "^3.3.4",
		"tslint": "^5.8.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.21"
	},
	"dependencies": {
		"xregexp": "^4.2.0"
	}
}